{
"name": "Analog Clock",
"developer": {"name" : "lonedev", "url" : ""},
"description" : "This is a Speed Dial extension showing a simple analog clock.",
"manifest_version" : 2,
"version" : "1.3",
"background" : {"page" : "index.html"},
"icons" : {"128": "64x64.png"},
"options_page" : "options.html",
"web_accessible_resources" : ["64x64.png","background.js","bgs/a.png","bgs/b.png","clock.js","style.css"],
"permissions" : ["https://*/*", "storage", "http://*/*"],
"speeddial" : {"url": "http://en.wikipedia.org/wiki/Time"},
"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'unsafe-eval';"
}
